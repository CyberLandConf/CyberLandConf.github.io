{% for page in site.pages reversed %}{% if page.date %}
{
"uid": "{{ page.date | date: "%Y%m%d" }}@ijug.eu",
"title": "{{ page.title | smartify }}",
"location": "{{ page.location }}",
"url": "{{ site.url }}{{ page.url }}",
